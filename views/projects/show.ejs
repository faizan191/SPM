    <a href="/home">Full List</a>
    <a href="/home/<%= foundProject._id %>/edit ">Edit</a>
    <form action="/home/<%= foundProject._id %>?_method=DELETE" method="post">
        <button>Delete</button>
    </form>
</body>
</html>